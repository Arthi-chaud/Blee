server {
	listen ${PORT} default_server;
	listen [::]:${PORT} default_server;
	access_log off;
	server_name _;

	location = /api {
	    return 302 /api/;
	}
	location /api/ {
	    proxy_pass ${SERVER_URL}/;
	}
	location = /transcoder {
	    return 302 /transcoder/;
	}
	location /transcoder/ {
	    proxy_pass ${TRANSCODER_URL}/;
	}
	location / {
		proxy_pass ${FRONT_URL};
	}
}